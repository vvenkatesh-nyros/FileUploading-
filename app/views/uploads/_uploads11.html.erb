<% @uploads1.each do |upload| %>
      <% if upload.filename =~ /^.*\.(jpg|png|gif|jpeg|JPG|PNG|GIF|JPEG)$/ %>
      
      <table>
      <tr>
          <td>
   
          <%= link_to image_tag("#{upload.file}",:class => 'lightbox_trigger11',:width => "50px",:height => "50px",:id => "#{upload.id}"), '#' %>
          </td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td class="name">
          <%= upload.filename %>
          </td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <% if session[:user]==upload.username %>
          <td>
          <%= link_to 'Delete', upload, method: :delete ,:class => "del1",:remote => true  %>
          </td>
          <% end %>
   
          

      </tr>
      </table>
     <% end %>
<% end %> 