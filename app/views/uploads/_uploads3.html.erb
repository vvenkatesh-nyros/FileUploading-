<% @uploads3.each do |upload| %>
   <% if upload.filename =~ /^.*\.(doc|docx)$/ %>
      <table>
      <tr>
          <td>
          <%= link_to image_tag("/assets/microw.png",:width => "50px",:height => "50px"),  "/#{upload.file}" ,:class => 'lightbox_trigger33' ,:id => "#{upload.id}" %>
          </td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td class="name">
          <%= upload.filename %>
          </td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td>
          <%= link_to 'Delete', upload, method: :delete,:class => "del",:remote => true %>
          </td>
      </tr>
      </table>  
   <% end %>  
<% end %>  